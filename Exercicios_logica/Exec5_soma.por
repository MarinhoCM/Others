// Elabore um aLgoritmo que leia dois 
// número e mostre a soma deles.
programa 
{
	funcao inicio()
	{
        real numero, outro_numero, soma
        escreva("infome um número: ")
        leia(numero)
        escreva("infome mais um número: ")
        leia(outro_numero)
        soma = (numero + outro_numero)
        escreva("A soma é: ", soma)
	}
}
